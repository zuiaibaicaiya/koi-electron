{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"]
    },
    "stub": {
      "dependsOn": ["^stub"]
    },
    "app-ui#build": {
      "cache": false,
      "dependsOn": ["koi-server#build"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
