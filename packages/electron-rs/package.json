{
  "name": "electron-rs",
  "version": "1.1.2",
  "description": "Electron build tooling based on rsbuild",
  "author": "最爱白菜吖<1355081829@qq.com>",
  "license": "MIT",
  "homepage": "https://www.zuiaibaicaiya.xyz/",
  "repository": "https://github.com/zuiaibaicaiya/create-electron-rs.git",
  "keywords": [
    "electron",
    "electron-rs",
    "create-electron-rs",
    "rsbuild-plugin",
    "electron-plugin",
    "plugin"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "stub": "rslib build",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@rslib/core": "^0.17.2",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "private": true,
  "dependencies": {
    "javascript-obfuscator": "^4.1.1",
    "webpack-obfuscator": "^3.5.1"
  }
